@model tarea.Models.Calcular

@{
    ViewBag.Title = "Dividir";
}

<h2>Dividir</h2>
@using (@Html.BeginForm())
{
    @Html.TextBoxFor(operador=>operador.a)<br />
    @Html.TextBoxFor(operador=>operador.b)<br />
    <input type="submit" value="DIVIDIR" />
    <p>El resultado es: @ViewBag.resultado</p>
   
}
